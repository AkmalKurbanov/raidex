    $(".calculate-delivery__item").each(function () {
    


          var $this = $(this);
          let lbs = 0.0022046
          let kg = 1000
          let inputWeight = $('.calculate-delivery__weight-kg input', $this);
          let totalPrice = $('.calculate-delivery__price', $this);
          let calculator = $('.calculate-delivery__calc', $this);
          let totalLbs = $('.calculate-delivery__weight-lbs span', $this);
          let priceKilo = $(this).find('.calculate-delivery__price-per-kilo').attr('data-price');
          let iconItem = $('.calculate-delivery__icon-item',$this);
          
          

          $(".calculate-delivery__calc", $this).slider({
            range: "min",
            value: 1,
            min: 0.7,
            max: 5.5,
            step: 0.01,
            slide: function (event, ui) {
              let weightKg = ui.value
              let kgToLbs = (weightKg * lbs * kg)
              let calculatedPrice = weightKg * priceKilo;
              inputWeight.val(weightKg)
              totalLbs.text(`(${kgToLbs.toFixed(2)} lbs)`);                  

              
              
              iconItem.each(function (index) {
                let one = $(this).children('.weight').attr('data-weight');
                let two = inputWeight.val();
                
                if (one <= two) {
                totalPrice.text(calculatedPrice.toFixed(2));  
                $(this).addClass('active').find('.icon svg path, .icon svg rect, .icon svg circle').each(function () {
                    if ($(this).attr('stroke')) {
                      $(this).attr('stroke', '#009688')
                    } else {
                      $(this).attr('fill', '#009688')
                    }
                  })
                $(this).parent().find('.calculate-delivery__icon-item.active').not($(this)).removeClass('active');
                $this.find('.calculate-delivery__icon-item').not($(this)).find('.icon svg path, .icon svg rect, .icon svg circle').each(function () {
                    if ($(this).attr('stroke')) {
                      $(this).attr('stroke', '#A3B1BB')
                    } else {
                      $(this).attr('fill', '#A3B1BB')
                    }
                  })
              }else if(one <= 1){ 
                totalPrice.text(priceKilo + '.00');  
              }
              else {
                $(this).removeClass('active')
              }
            
              
              });
              

              $(".tabs_item-js.active .calculator-js").each(function (index, value) {
                  var calculatorSlider = new Swiper(value, {
                    loop: false,
                    speed: 1000,
                    spaceBetween: 20,
                    
                    breakpoints: {
                      0: {
                        slidesPerView: 7,
                        centeredSlides: true, 
                        spaceBetween: 24
                        },
                      400: {
                        slidesPerView: 7,
                        centeredSlides: true, 
                        spaceBetween: 43
                        },
                      576: {
                        slidesPerView: 8,
                        centeredSlides: true,
                        spaceBetween: 45
                      },
                      768: {
                        slidesPerView: 10,
                        centeredSlides: true,
                        spaceBetween: 43
                      },
                      992: {
                        slidesPerView: 16,
                        centeredSlides: false,
                        spaceBetween: 16,
                      },
                      1200: {
                        slidesPerView: 16,
                        centeredSlides: false
                      },
                    }
                  });
                  let calculatorActiveSlide = $(this).find(".calculate-delivery__icon-item.active").attr("aria-label");
                  $(this).find(".calculate-delivery__icon-item.active").addClass('swiper-slide-active');               
                  

                  
                  if(calculatorActiveSlide == 1) {
                    calculatorSlider.slideTo(1, 500);
                    var conditionElement = true
                    $(this).find('.swiper-wrapper').css('transform', 'translate3d(0, 0px, 0px)!important');
                  }
                  else{
                    calculatorSlider.slideTo(calculatorActiveSlide, 500);
                  }

                  
                });

                $('.swiper-notification').remove();
              


            }

          });

          $('.calculate-delivery__icon-item.active').find('.icon svg path, .icon svg rect, .icon svg circle').each(function () {
            if ($(this).attr('stroke')) {
              $(this).attr('stroke', '#009688')
            } else {
              $(this).attr('fill', '#009688')
            }
          })
                  
          let minWeightKg = calculator.slider("value")
          let minWeightlbs = calculator.slider("value") * lbs
          let kgToLbs = (minWeightKg * lbs * kg)        
          let calculatedPrice = minWeightKg * priceKilo;


          inputWeight.val($(".calculate-delivery__calc", $this).slider("value"));
          totalLbs.text(`(${kgToLbs.toFixed(2)} lbs)`);
          totalPrice.text(calculatedPrice.toFixed(2));

          iconItem.on('click', function () {
            let iconWeight = $(this).find('.weight').attr('data-weight');
            let iconPrice = iconWeight * priceKilo
            let iconKgToLbs = (iconWeight * lbs * kg)

            
            
            calculator.slider("value", iconWeight);
            inputWeight.val(iconWeight)
            
            totalLbs.text(`(${iconKgToLbs.toFixed(2)} lbs)`);
            


            if (iconWeight <= 1){
                totalPrice.text(priceKilo + ".00");
              }else{
                totalPrice.text(iconPrice.toFixed(2));
              }



          });

          iconItem.each(function (index) {

            let one = $(this).children('.weight').attr('data-weight');
            let two = inputWeight.val();
            let three = $(this).next().find('.weight').attr('data-weight');
            
            if (one == two) {$(this).addClass('active')} else {$(this).removeClass('active')}
          });

          inputWeight.on('focus', function (){
            $(this).val('');
            iconItem.each(function (index) {
              
              $(this).removeClass('active').find('.icon svg path, .icon svg rect, .icon svg circle').each(function () {
                if ($(this).attr('stroke')) {
                  $(this).attr('stroke', '#A3B1BB')
                } else {
                  $(this).attr('fill', '#A3B1BB')
                }
                totalPrice.text("0.00");
              })
          
            });
          })
          inputWeight.on('keyup', function () {
            calculator.slider("value", $(this).val());
            let lastItem = +$this.find('.calculate-delivery__icon-item:last .weight').attr('data-weight');
            totalLbs.text(`(${($(this).val() * lbs * kg).toFixed(2)} lbs)`);


            iconItem.each(function (index) {
              
              let one = $(this).children('.weight').attr('data-weight');
              let two = inputWeight.val();
              
              if (one <= two) {
                $(this).addClass('active').find('.icon svg path, .icon svg rect, .icon svg circle').each(function () {
                    if ($(this).attr('stroke')) {
                      $(this).attr('stroke', '#009688')
                    } else {
                      $(this).attr('fill', '#009688')
                    }
                  })
                $(this).parent().find('.calculate-delivery__icon-item.active').not($(this)).removeClass('active');
                $this.find('.calculate-delivery__icon-item').not($(this)).find('.icon svg path, .icon svg rect, .icon svg circle').each(function () {
                    if ($(this).attr('stroke')) {
                      $(this).attr('stroke', '#A3B1BB')
                    } else {
                      $(this).attr('fill', '#A3B1BB')
                    }
                  })
              } 
              else {
                $(this).removeClass('active')
              }


            });

              if ($(this).val() == '') {
                  
                  iconItem.removeClass('active').find('.icon svg path, .icon svg rect, .icon svg circle').each(function () {
                if ($(this).attr('stroke')) {
                  $(this).attr('stroke', '#A3B1BB')
                } else {
                  $(this).attr('fill', '#A3B1BB')
                }
                totalPrice.text("0.00");
              })
              } else if ($(this).val() <= 1){
                totalPrice.text(priceKilo + ".00");
              }
              else if($(this).val() > Number($('.last-item-weight.active .weight').attr('data-weight'))) {
                totalPrice.text(`${($(this).val() * priceKilo).toFixed(2)}`);
                
                $('.last-item-weight.active').removeClass('active');
                $('.calculate-delivery__icon-item').not($(this)).find('.icon svg path, .icon svg rect, .icon svg circle').each(function () {
                    if ($(this).attr('stroke')) {
                      $(this).attr('stroke', '#A3B1BB')
                    } else {
                      $(this).attr('fill', '#A3B1BB')
                    }
                  })
              }
              else{
                totalPrice.text(`${($(this).val() * priceKilo).toFixed(2)}`);
              }


            

            

      });
    
    });

