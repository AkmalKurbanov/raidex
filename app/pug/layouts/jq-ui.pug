  include ../includes/preloader
  if backgroundColor == 'gray'
    .wrapper.wrapper_bg-gray#top
      .content
        include ../includes/menu
        include ../includes/header

        .content__inner
          block main
        

      section.section
        include ../includes/faq

      include ../includes/additional-info
      include ../includes/footer

  else
    .wrapper#top
      .content
        include ../includes/menu
        include ../includes/header

        .content__inner
          block main
        

      section#faq
        include ../includes/faq

      include ../includes/additional-info
      include ../includes/footer    
  
